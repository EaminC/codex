export  FORGE_API_KEY=YOUR_FORGE_API_KEY

codex exec \
  -c "model_providers.forge={ name='Forge', base_url='https://api.forge.tensorblock.co/v1', wire_api='responses', env_key='FORGE_API_KEY' }" \
  -c "model_provider='forge'" \
  -c "model='tensorblock/gpt-4.1-mini'" \
  --yolo \
  "hello"


  python3 /home/cc/codex/Baseline/run_pipeline.py --repos-root /home/cc/codex/Baseline/repo